
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/admin_Layout.cshtml";
}
<p>
    @Html.ActionLink("新建WebURI", "Create")
</p>
<table class="table">
    @{int i = 0;}
    @foreach (var item in ViewBag.WebList)
    {
        <tr>
            <td>
                <p hidden>NetCount: @item.NetCount</p>
                @(++i)、  &nbsp; 链接: @item.Link &nbsp; 代号: @item.NickName
            </td>
            <td>
                @Html.ActionLink("编辑WebURI", "Edit", new { item.NetCount, item.Link, item.NickName })|
                @Html.ActionLink("删除WebURI", "Delete",new { item.NetCount, item.Link })
            </td>
        </tr>
    }
</table>

<!--[if !IE]> -->
<script src="~/Scripts/jquery-2.1.4.min.js"></script>
